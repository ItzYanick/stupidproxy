{
  "name": "stupidproxy",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "server:dev": "concurrently --restart-tries -1 \"bun --bun run frontend:dev\" \"bun --bun run backend:dev\"",
    "frontend:dev": "vite",
    "frontend:build": "tsc && vite build",
    "frontend:preview": "vite preview",
    "frontend:clean": "rm -rf dist",
    "backend:dev": "bun --watch src/backend/main.ts",
    "backend:build": "bun build src/backend/main.ts --outdir ./bin --target bun",
    "backend:clean": "rm -rf db.sqlite && rm -rf _bin",
    "client:generate": "cd src/client && go generate",
    "client:build": "go build -o _dist/client src/client/main.go && strip _dist/client",
    "client:dist": "bun run client:generate && bun run client:build",
    "client:clean": "rm -rf _dist && rm -rf src/client/_bin",
    "download:rathole": "./scripts/download-rathole.sh",
    "download:caddy": "./scripts/download-caddy.sh",
    "clean": "bun run frontend:clean && bun run backend:clean && bun run client:clean"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "concurrently": "^8.2.2",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.307.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "smol-toml": "^1.1.3",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/bun": "^1.0.0",
    "@types/decompress": "^4.2.7",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/web": "^0.0.134",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
